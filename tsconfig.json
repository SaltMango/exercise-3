{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "src",
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators":true,
    "lib": ["dom", "es2015"]
    
    
  },
  "include": ["src"]
}
